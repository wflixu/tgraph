{
  "name": "tgraph",
  "version": "0.1.0",
  "description": "a graph editor render with svg base from mxgraph",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "demos": "pnpm --filter=demos  dev",
    "build": "pnpm  --filter=thgraph  run build",
    "docs:dev": "pnpm --filter docs run docs:dev",
    "docs:build": "pnpm --filter docs run docs:build",
    "next:dev": "pnpm run dev --filter thgraph-next-demo",
    "next:build": "pnpm --filter=thgraph-next run build ",
    "pub:doc": "zx ./script/docs.mjs"
  },
  "devDependencies": {
    "zx": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wflixu/tgraph.git"
  },
  "keywords": [
    "mxGraph",
    "jgraph",
    "draw.io",
    "svg",
    "editor",
    "es6",
    "TypeScript"
  ],
  "author": "wflixu",
  "license": "MIT"
}

